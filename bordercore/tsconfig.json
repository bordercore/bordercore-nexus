{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "jsx": "react-jsx",
    "moduleResolution": "Node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["front-end/*"]
    }
  },
  "include": ["front-end/**/*.ts", "front-end/**/*.tsx"],
  "exclude": ["node_modules"]
}
